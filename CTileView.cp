// CTileView.cp#include "CTileView.h"#include <assert.h>CTileView::CTileView(LStream *inStream)	: LOffscreenView(inStream) {		mSpecial = 3;}void CTileView::FinishCreateSelf() {	ChangeSpecial();}void CTileView::ChangeSpecial() {	mSpecial == mSubPanes.GetCount() ? mSpecial = 1 : mSpecial++;	CTile* tile = NULL;	LPane* pane = NULL;	mSubPanes.FetchItemAt(mSpecial, pane);	assert(pane != NULL);	tile = (CTile*) pane; // separate out the cast		LView* view = NULL; 	view = GetSuperView();	assert(view != NULL);	pane = view->FindPaneByID(100);		tile->UpdateCaption((LCaption*) pane);}void CTileView::TileWasClicked(class CTile* tile) {	if (mSubPanes.FetchIndexOf(tile) == mSpecial) tile->SetWinner(true);} 